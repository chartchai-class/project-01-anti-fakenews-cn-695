{
  "name": "fakenews",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build -- --mode analyze",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "lint": "eslint . --ext .vue,.ts,.js",
    "lint:fix": "eslint . --ext .vue,.ts,.js --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"./**/*.{vue,ts,js,json}\"",
    "format:check": "prettier --check \"./**/*.{vue,ts,js,json}\"",
    "validate": "npm run lint && npm run typecheck && npm run format:check",
    "clean": "rimraf dist node_modules/.vite",
    "reinstall": "npm run clean && npm ci",
    "deploy:vercel": "vercel --prod",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "vue": "^3.5.12",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.4.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^6.0.0",

    "vue-eslint-parser": "^10.2.0"
  }
}
